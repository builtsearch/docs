{
	"useTabs": true,
	"singleQuote": false,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": [
		"prettier-plugin-svelte"
	],
	"bracketSameLine": true,
	"svelteIndentScriptAndStyle": false,
	"svelteAllowShorthand": true,
	"svelteStrictMode": false,
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}